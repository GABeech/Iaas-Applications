{
  "layouts": [
    {
      "layout_name": "default_titandb_dashboard",
      "display_name": "Standard TitanDB Dashboard",
      "section_name": "TITANDB_SUMMARY",
      "widgetLayoutInfo": [
        {
          "widget_name": "Writes",
          "description": "Count of write requests on all TSDs in the cluster.",
          "widget_type": "GRAPH",
          "is_visible": true,
          "metrics": [
            {
              "name": "tsd.rpc.received.telnet._sum",
              "metric_path": "metrics/tsd/rpc/received/telnet._sum",
              "service_name": "TITANDB",
              "component_name": "TITANDB_SERVER"
            },
            {
              "name": "tsd.rpc.received.http._sum",
              "metric_path": "metrics/tsd/rpc/received/http._sum",
              "service_name": "TITANDB",
              "component_name": "TITANDB_SERVER"
            }
          ],
          "values": [
            {
              "name": "Write Requests",
              "value": "${tsd.rpc.received.telnet._sum + tsd.rpc.received.http._sum}"
            }
          ],
          "properties": {
            "graph_type": "LINE",
            "time_range": "1"
          }
        }
      ]
    }
  ]
}
